{
  "manifest_version": 3,
  "name": "Auto Clicker Extension",
  "version": "1.0",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["https://onprover.orochi.network/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "Auto Clicker"
  }
}
